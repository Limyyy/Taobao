<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:orientation="vertical"
    android:layout_height="wrap_content">
    <LinearLayout
        android:background="@drawable/all_list_child_item_stroke"
        android:layout_width="518dp"
        android:orientation="horizontal"
        android:layout_margin="5dp"
        android:layout_height="160dp">
        <ImageView
            android:id="@+id/all_list_child_item_image"
            android:layout_marginLeft="8dp"
            android:layout_marginTop="8dp"
            android:layout_marginBottom="8dp"
            android:layout_width="140dp"
            android:layout_height="140dp" />
        <LinearLayout
            android:orientation="vertical"
            android:layout_margin="8dp"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <TextView
                android:id="@+id/all_list_child_item_name"
                android:layout_width="match_parent"
                android:layout_height="80dp" />
            <android.support.v4.widget.Space
                android:layout_weight="1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
            <LinearLayout
                android:layout_marginLeft="8dp"
                android:layout_marginBottom="8dp"
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <TextView
                    android:textColor="#f00"
                    android:text="ï¿¥:0"
                    android:id="@+id/all_list_child_item_price"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
                <android.support.v4.widget.Space
                    android:layout_width="wrap_content"
                    android:layout_weight="1"
                    android:layout_height="wrap_content" />
                <com.gx.tianba.fragment.shopping.myview.MyView
                    android:id="@+id/all_list_child_item_jiajian"
                    android:layout_width="wrap_content"
                    android:layout_marginRight="30dp"
                    android:layout_height="wrap_content"></com.gx.tianba.fragment.shopping.myview.MyView>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</LinearLayout>